<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="#aaa"/>
            <corners
                android:radius="10dp" />

            <padding
                android:left="1dp"
                android:right="1dp"
                android:top="1dp"
                android:bottom="1dp"/>

            <solid android:color="#26A65B"/>
        </shape>
    </item>
</selector>